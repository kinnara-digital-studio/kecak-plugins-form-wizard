[
    {
        title:'@@form.formWizard.edit@@',
        properties:[
            {
                name:'id',
                label:'@@form.formWizard.id@@',
                type:'textfield',
                required:'True',
                regex_validation : '^[a-zA-Z0-9_]+$',
                validation_message : '@@form.formWizard.invalidId@@'
            },
            {
                name:'label',
                label:'@@form.formWizard.label@@',
                type:'textfield'
            },
            {
                name:'numberOfPage',
                label:'@@form.formWizard.numberOfPage@@',
                type : 'elementselect',
                value : '2',
                options : [
                    {value : '2' , label : '2'},
                    {value : '3' , label : '3'},
                    {value : '4' , label : '4'},
                    {value : '5' , label : '5'},
                    {value : '6' , label : '6'},
                    {value : '7' , label : '7'},
                    {value : '8' , label : '8'},
                    {value : '9' , label : '9'},
                    {value : '10' , label : '10'},
                    {value : '11' , label : '11'},
                    {value : '12' , label : '12'},
                    {value : '13' , label : '13'},
                    {value : '14' , label : '14'},
                    {value : '15' , label : '15'}
                ],
                url : '[CONTEXT_PATH]/web/json/app[APP_PATH]/plugin/com.kecak.enterprise.MultiPagedForm/service?action=getJson',
                keep_value_on_change : 'true'
            }
        ]
    },
    {
        title:'@@form.formWizard.advancedOptions@@',
        properties:[
            {
                label : '@@form.formWizard.data@@',
                type : 'header'
            },
            {
                name:'partiallyStore',
                label:'@@form.formWizard.partiallyStore@@',
                type:'checkbox',
                value:'false',
                options:[
                    {
                        value:'true',
                        label:''
                    }
                ]
            },
            {
                name:'storeMainFormOnPartiallyStore',
                label:'@@form.formWizard.storeMainFormOnPartiallyStore@@',
                type:'checkbox',
                value:'true',
                control_field: 'partiallyStore',
                control_value: 'true',
                options:[
                    {
                        value:'true',
                        label:''
                    }
                ]
            },
            {
                label : '@@form.formWizard.ui@@',
                type : 'header'
            },
            {
                name:'displayMode',
                label:'@@form.formWizard.displayMode@@',
                type:'selectbox',
                value : 'wizard',
                options : [
                    {
                        value : 'wizard',
                        label : '@@form.formWizard.displayMode.wizard@@'
                    },
                    {
                        value : 'tab',
                        label : '@@form.formWizard.displayMode.tab@@'
                    }
                ]
            },
            {
                name:'prevButtonlabel',
                label:'@@form.formWizard.prevButtonlabel@@',
                value : 'Prev',
                type:'textfield'
            },
            {
                name:'nextButtonlabel',
                label:'@@form.formWizard.nextButtonlabel@@',
                value : 'Next',
                type:'textfield'
            },
            {
                name:'onlyAllowSubmitOnLastPage',
                label:'@@form.formWizard.onlyAllowSubmitOnLastPage@@',
                type:'checkbox',
                value:'false',
                options:[
                    {
                        value:'true',
                        label:''
                    }
                ]
            },
            {
                name : 'css',
                label : '@@form.formWizard.css@@',
                type : 'textarea',
                cols : '60',
                rows : '15'
            }
        ]
    }
]